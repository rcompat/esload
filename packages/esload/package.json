{
  "name": "esload",
  "version": "0.1.0",
  "description": "JavaScript module loader",
  "bugs": "https://github.com/rcompat/esload/issues",
  "license": "MIT",
  "files": [
    "/src/**/*.js",
    "/src/**/*.d.ts",
    "!/**/*.spec.*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rcompat/rcompat",
    "directory": "packages/esload"
  },
  "scripts": {
    "test": "npx debris"
  },
  "dependencies": {
    "@esload/core": "workspace:^",
    "@rcompat/fs": "^0.4.0",
    "@rcompat/invariant": "^0.5.0",
    "@rcompat/object": "^0.5.0",
    "@rcompat/package": "^0.3.0"
  },
  "type": "module",
  "imports": {
    "#*": {
      "@esload/lt": "./src/private/*.ts",
      "default": "./src/private/*.js"
    },
    "#loader/*": {
      "@esload/lt": "./src/private/loader/*/index.ts",
      "default": "./src/private/loader/*/index.js"
    }
  },
  "exports": {
    ".": "./src/index.js",
    "./register": "./src/register.js",
    "./test": "./src/test.js"
  }
}
